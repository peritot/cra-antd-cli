{
  "name": "cra-antd-cli",
  "version": "1.0.0",
  "description": "A CLI Tool for create a react app with antd",
  "keywords": [
    "cli",
    "react",
    "antd"
  ],
  "author": {
    "name": "peritot",
    "email": "peritotchan@gmail.com"
  },
  "homepage": "https://github.com/peritot/cra-antd-cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peritot/cra-antd-cli"
  },
  "license": "MTI",
  "main": "index.js",
  "bin": {
    "cra-antd-cli": "bin/cra-antd-cli.js"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {},
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "**/*.{css,less,scss,html,json,md,markdown},": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.2.1",
    "download-git-repo": "^3.0.2",
    "ejs": "^3.1.5",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "inquirer": "^7.3.3",
    "ora": "^5.1.0",
    "sanitize-filename": "^1.6.3"
  }
}
